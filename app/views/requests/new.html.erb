<% provide(:title, "new subject") %>
<h1>New Subject</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@request, html: {multipart: true}) do |f| %>
      <%= render "shared/error_messages", object: f.object %>
      
      <%= f.select :class_student_id, options_from_collection_for_select(ClassStudent.all,"id","name"), class: "form-control" %>

      <%= f.submit "Create subject", class: "btn btn-primary" %> 
    <% end %>
  </div>
</div>
